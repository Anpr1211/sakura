---
layout: null
---
[
  {% for post in site.posts %}
    {
    
      "title"    : "{{ post.title | strip_html | escape }}",
      "category" : "{{ post.category }}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "summary"  : "{{ post.summary | strip_html | strip_newlines | escape }}",
      "excerpt"  : "{{ post.excerpt | strip_html | strip_newlines | escape }}",
      "date"     : "{{ post.date }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "image"    : "{{ post.image }}"
      
    },
  {% endfor %}
  {% for page in site.html_pages %} {% unless page.title == "" or page.title == "Not found" or page.title == "Archive" or page.title == "Documentation" %}
    {
    
      "title"    : "{{ page.title | strip_html | escape }}",
      "excerpt"  : "{{ page.content | strip_html | strip_newlines | truncatewords: 50 | escape }}",
      "url"      : "{{ site.baseurl }}{{ page.url }}"
      
    },
  {% endunless %} {% endfor %}

  {% for item in site.docs %}
    {
    
      "title"    : "{{ item.title | strip_html | escape }}",
      "category" : "{{ item.category }}",
      "tags"     : "{{ item.tags | join: ', ' }}",
      "summary"  : "{{ item.summary | strip_html | strip_newlines | escape }}",
      "excerpt"  : "{{ item.excerpt | strip_html | strip_newlines | escape }}",
      "date"     : "{{ item.date }}",
      "url"      : "{{ site.baseurl }}{{ item.url }}",
      "image"    : "{{ item.image }}"
      
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]